{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch index.tsx",
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "webRoot": "${workspaceFolder}/client/src",
            "url": "http://localhost:3000",
            "pathMappings": [
                { "url": "webpack:///client/src/*", "path": "${webRoot}/client//*" }
            ]
        }
    ]
}